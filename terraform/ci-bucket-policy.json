{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "putfiles",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::${bucket_name}/*",
        "arn:aws:s3:::${bucket2_name}/*",
        "arn:aws:s3:::${bucket3_name}/*"
      ]
    }
  ]
}
